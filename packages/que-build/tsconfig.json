{
  "include": ["src", "env.d.ts"],
  "exclude": ["**/*.spec.ts"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2019",
    "module": "ES2020",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "types": ["bun-types"],
    "paths": {
      "@quejs/esbuild-plugin-adapter": ["../esbuild-plugin-adapter"]
    }
  }
}
